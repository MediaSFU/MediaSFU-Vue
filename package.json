{
  "name": "mediasfu-vue",
  "version": "1.0.1",
  "private": false,
  "description": "MediaSFU Prebuilt Vue 3 SDK - Pre-built video conferencing components for Vue applications",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/mediasfu-vue.css": "./dist/mediasfu-vue.css",
    "./dist/style.css": "./dist/mediasfu-vue.css"
  },
  "files": [
    "dist/",
    "src/",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:lib": "vite build --mode production && bash scripts/create-types.sh",
    "build-docs": "typedoc",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.vue",
    "lint:fix": "eslint . --ext .ts,.vue --fix",
    "prepublishOnly": "npm run build:lib && npm run build-docs",
    "prepack": "npm run build:lib"
  },
  "keywords": [
    "mediasfu",
    "vue",
    "vue3",
    "prebuilt",
    "webRTC",
    "video",
    "audio",
    "chat",
    "media",
    "sfu",
    "MediaSFU",
    "cloud recording",
    "meeting",
    "conference",
    "conferencing",
    "broadcasting",
    "webinar",
    "recording",
    "streaming",
    "websocket",
    "communication",
    "real-time",
    "web",
    "sdk",
    "vuejs",
    "mediasoup",
    "screen sharing",
    "screen sharing annotation",
    "collaborative whiteboard",
    "webRTC recording",
    "webRTC streaming",
    "mediasfu-vue",
    "mediasfu-vue-sdk",
    "mediasfu-vue-prebuilt"
  ],
  "author": "MediaSFU",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MediaSFU/MediaSFU-Vue"
  },
  "bugs": {
    "url": "https://github.com/MediaSFU/MediaSFU-Vue/issues",
    "email": "info@mediasfu.com"
  },
  "homepage": "https://github.com/MediaSFU/MediaSFU-Vue#readme",
  "peerDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0 || ^7.0.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0 || ^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0 || ^7.0.0",
    "@fortawesome/vue-fontawesome": "^3.1.0",
    "@mediapipe/selfie_segmentation": "^0.1.1675465747",
    "mediasoup-client": "^3.16.0",
    "socket.io-client": "^4.8.0",
    "vue": "^3.4.0 || ^3.5.0"
  },
  "dependencies": {
    "@fortawesome/react-fontawesome": "^3.1.0",
    "bootstrap": "^5.3.8",
    "mediasfu-shared": "^1.0.1",
    "universal-cookie": "^8.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/vue-fontawesome": "^3.1.2",
    "@mediapipe/selfie_segmentation": "^0.1.1675465747",
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.13.0",
    "eslint-plugin-vue": "^9.28.0",
    "mediasoup-client": "^3.16.7",
    "socket.io-client": "^4.8.1",
    "typedoc": "^0.26.11",
    "typedoc-plugin-extras": "^3.1.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.8.1",
    "vite": "^7.1.7",
    "vue": "^3.5.22",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.0"
  },
  "overrides": {
    "typedoc": {
      "typescript": "$typescript"
    }
  }
}
